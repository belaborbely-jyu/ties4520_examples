<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE rdf:RDF [
  <!ENTITY rdf   "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs  "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd   "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY owl   "http://www.w3.org/2002/07/owl#">
  <!ENTITY sswap "http://sswapmeet.sswap.info/sswap/">
  <!ENTITY mime  "http://sswapmeet.sswap.info/mime/">
]>

<rdf:RDF
	xmlns:rdf   = "&rdf;"
	xmlns:rdfs  = "&rdfs;"
	xmlns:xsd   = "&xsd;"
	xmlns:owl   = "&owl;"
	xmlns:sswap = "&sswap;"
	xmlns:mime  = "&mime;"
  >

  <sswap:Resource>

    <sswap:operatesOn>
      <sswap:Graph>
        <sswap:hasMapping>
          <sswap:Subject>
          <rdf:type rdf:resource="&mime;image/Png"/>
            <sswap:mapsTo>
              <sswap:Object/>
            </sswap:mapsTo>
          </sswap:Subject>
        </sswap:hasMapping>
      </sswap:Graph>
    </sswap:operatesOn>

  </sswap:Resource>

</rdf:RDF>
